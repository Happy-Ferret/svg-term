{
  "name": "svg-term",
  "version": "1.0.6",
  "description": "Share terminal sessions via SVG and CSS",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "clean": "npx rimraf lib",
    "deps": "npm run build && dependency-check . --missing && dependency-check . --extra --no-dev",
    "test": "ava"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marionebl/svg-term.git"
  },
  "keywords": [
    "asciinema",
    "asciicast",
    "svg",
    "css"
  ],
  "author": "Mario Nebl <hello@mario-nebl.de>",
  "bugs": {
    "url": "https://github.com/marionebl/svg-term/issues"
  },
  "homepage": "https://github.com/marionebl/svg-term#readme",
  "ava": {
    "files": [
      "src/*.test.js",
      "fixtures/*.json"
    ],
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "ignore": "*.test.js",
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 8
          }
        }
      ],
      "react"
    ]
  },
  "devDependencies": {
    "@marionebl/sander": "^0.6.1",
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "copy-paste": "^1.3.0",
    "dependency-check": "^2.9.2",
    "pkg-dir": "^2.0.0",
    "xmldom": "^0.1.27"
  },
  "dependencies": {
    "@stiligita/constants": "^1.0.0-0",
    "@stiligita/core": "^1.0.0-0",
    "@stiligita/keyframes": "^1.0.0-0",
    "@stiligita/react": "^1.0.0-0",
    "@stiligita/stylesheets": "^1.0.0-0",
    "abcq": "^0.2.2",
    "ansi-to-rgb": "^1.0.0",
    "load-asciicast": "^1.0.1",
    "lodash": "^4.17.4",
    "object-hash": "^1.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "tag-hoc": "^1.0.0"
  }
}
